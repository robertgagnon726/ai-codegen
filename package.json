{
  "name": "aicodegen",
  "version": "1.0.0",
  "main": "dist/bin/cli.js",
  "description": "AI-based code generation tool",
  "type": "module",
  "bin": {
    "aicodegen": "dist/bin/cli.js"
  },
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "fix-extensions": "node internals/fix-imports.internal.js",
    "postbuild": "npm run fix-extensions",
    "prepublishOnly": "npm run build",
    "start": "npm run build && node dist/bin/cli.js",
    "clean": "rm -rf dist",
    "commit": "aicommits --generate 5 --type conventional",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint ."
  },
  "files": [
    "dist",
    "README.md",
    "internals/aicodegen-schema.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robertgagnon726/ai-codegen.git"
  },
  "keywords": [
    "AI",
    "jest"
  ],
  "author": "Bobby Gagnon",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "diff": "^7.0.0",
    "gpt-3-encoder": "^1.1.4",
    "openai": "^4.65.0",
    "ora": "^8.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^8.57.1",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/github": "^11.0.0",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@types/node": "^22.7.4",
    "@vitest/coverage-istanbul": "^2.1.1",
    "aicommits": "^1.11.0",
    "coveralls": "^3.1.1",
    "eslint": "^8.57.1",
    "globals": "^15.9.0",
    "replace-in-file": "^8.2.0",
    "semantic-release": "^24.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "typescript-eslint": "^7.18.0",
    "vite": "^5.4.8",
    "vitest": "^2.1.1"
  }
}
